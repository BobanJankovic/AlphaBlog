<h1>LIsting all categories:</h1>
<div class="col-sm-4 offset-sm-4" >
  <%= will_paginate  %>
</div>
<div class="row ">
  <% @categories.each do |category| %>
    <div class="card col-sm-8 bg-secondary marg bg-info offset-sm-2">
    <div class="card-body">
      <%= link_to "#{category.name}", category_path(category) %>
      </div>
      </div>
        
    <% end %>
    
  
</div>
