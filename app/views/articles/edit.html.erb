<h1>Update new article </h1>
<%= render 'form' %>


 
 

<%= form_for @article do |f| %>
<p>
    <%= f.label :title %> <br/>
    <%= f.text_field :title %> <br/><br/>
    <%= f.label :description %> <br/>
    <%= f.text_area :description %> <br/><br/>
     <%= f.submit "Update" %> 



 </p>

<% end %>